<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SellIt_UWP">

    <SolidColorBrush x:Key="green" Color="#558c2f"/>
    <SolidColorBrush x:Key="green-light" Color="#85bc5c"/>
    <SolidColorBrush x:Key="green-dark" Color="#255e00"/>
    <SolidColorBrush x:Key="purple" Color="#280593"/>
    <SolidColorBrush x:Key="purple-light" Color="#5f5fc4"/>
    <SolidColorBrush x:Key="purple-dark" Color="#001064"/>

    <Style TargetType="Button">
        <Setter Property="Background" Value="{StaticResource purple}" />
        <Setter Property="Foreground" Value="White" />
        
    </Style>

    <Style TargetType="TextBlock">
        
    </Style>
    
</ResourceDictionary>
